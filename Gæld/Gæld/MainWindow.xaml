<Window x:Class="Gæld.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local1="clr-namespace:Gæld"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="14.333"/>
            <RowDefinition Height="29.667"/>
            <RowDefinition Height="57"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>

            <ColumnDefinition Width="107*"/>
            <ColumnDefinition Width="156*"/>
            <ColumnDefinition Width="66*"/>
            <ColumnDefinition Width="65*"/>
            <ColumnDefinition Width="399*"/>

        </Grid.ColumnDefinitions>
        <TextBlock Text="Gæld:" Grid.Row="1" Grid.Column="0" FontSize="18" HorizontalAlignment="Center" Margin="31,0.333,29.333,57.333" Grid.RowSpan="2"  />
        <Button  Content="Tilføj Gæld" Grid.Column="3" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center" Height="20" Margin="1.667,5.333,2.333,5.333" Width="62" />
        <ListBox Grid.Row="1" Name="listen" Grid.ColumnSpan="2" Margin="107,30.333,0.333,0.333" Grid.RowSpan="2"/>
        <StackPanel x:Name="nyDebt" HorizontalAlignment="Center" Height="100" Width="300" VerticalAlignment="Top" IsEnabled="False">
            <TextBlock Text="Heeeeey"/>
        </StackPanel>
    </Grid>
</Window>
